body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000000;
	background-color: #FFFFFF;
	scrollbar-face-color: #9FB9D6;
	scrollbar-shadow-color: #FEFFFF;
	scrollbar-highlight-color: #FEFFFF;
	scrollbar-3dlight-color: #FEFFFF;
	scrollbar-darkshadow-color: #E3ECF4;
	scrollbar-track-color: #FEFFFF;
	scrollbar-arrow-color: #072191;
}
a:link {
	color: #000000;
	text-decoration: none;
}
a:visited {
	color: #000000;
	text-decoration: none;
}
a:hover {
	color: #0A79D0;
	text-decoration: underline;
}
a:active {
	color: #ff0000;
	text-decoration: none;
}
a.llink:link {
	color: #3D5A92;
	text-decoration: none;
}
a.llink:visited {
	color: #3D5A92;
	text-decoration: none;
}
a.llink:hover {
	color: #000000;
	text-decoration: underline;
}
a.llink:active {
	color: #3D5A92;
	text-decoration: none;
}
a.wlink:link {
	color: #176997;
	text-decoration: none;
}
a.wlink:visited {
	color: #176997;
	text-decoration: none;
}
a.wlink:hover {
	color: #61ACE7;
	text-decoration: underline;
}
a.wlink:active {
	color: #176997;
	text-decoration: none;
}
