body {
	font-size: 12px;
	margin: 0;
	padding: 5px;
	color: #000000;
	background-color: #D7EBFD;
	scrollbar-face-color: #9FB9D6;
	scrollbar-shadow-color: #FEFFFF;
	scrollbar-highlight-color: #FEFFFF;
	scrollbar-3dlight-color: #FEFFFF;
	scrollbar-darkshadow-color: #E3ECF4;
	scrollbar-track-color: #FEFFFF;
	scrollbar-arrow-color: #072191;
}


a:link {
	color: #000000;
	text-decoration: underline;
}

a:hover {
	color: #3B4F6A;
	text-decoration: none;
}

a:visited {
	color: #3B4F6A;
	text-decoration: underline;
}

a:active {
	color: #3B4F6A;
	text-decoration: underline;
}

a.llink:link {
	color: #3D5A92;
	text-decoration: underline;
}

